{
  "name": "test-network-mycontext",
  "version": "1.0.0",
  "client": {
    "organization": "MyContext",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "Org${ORG}": {
      "mspid": "MyContextMSP",
      "peers": ["peer0.mycontext.example.com"],
      "certificateAuthorities": ["ca.mycontext.example.com"]
    }
  },
  "peers": {
    "peer0.org${ORG}.example.com": {
      "url": "grpcs://localhost:${P0PORT}",
      "tlsCACerts": {
        "pem": "${PEERPEM}"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.mycontext.example.com",
        "hostnameOverride": "peer0.mycontext.example.com"
      }
    }
  },
  "certificateAuthorities": {
    "ca.org${ORG}.example.com": {
      "url": "https://localhost:${CAPORT}",
      "caName": "ca-mycontext",
      "tlsCACerts": {
        "pem": ["${CAPEM}"]
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
